      recolorable		= TRUE
      detail			= "entry"
      file			= "Images/Shadows/entry-focus-panel-blue.png"
      border			= {3, 3, 4, 3}
      stretch			= TRUE
    }	

    # the actual textbox in normal state
    image
    {
     	function		= SHADOW
    	detail			= "entry"
    	shadow			= IN
	state			= NORMAL
    	recolorable		= FALSE
    	file			= "Images/Shadows/entry-shadow-in-panel-blue.png"
    	border			= { 2,2,2,2 }
    	stretch			= TRUE
    }  
 }
