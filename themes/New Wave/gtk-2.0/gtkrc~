     	function		= SHADOW
    	detail			= "entry"
    	shadow			= IN
    	recolorable		= FALSE
    	file			= "Images/Combo/combo-entry.png"
    	border			= { 2,2,2,2 }
    	stretch			= TRUE
    }

   
    
  }
}

