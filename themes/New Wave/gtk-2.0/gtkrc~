  {

    image
    {
	function		= FOCUS
	recolorable		= TRUE
	detail 			= "entry"
	file			= "Images/Combo/combo-entry-focus.png"
	border			= {2, 2, 2, 3}
	stretch			= TRUE
    }	
    
	
    image
    {
      function			= BOX
      recolorable		= TRUE
