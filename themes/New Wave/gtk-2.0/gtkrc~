      border			= { 3,3,4,3 }
      stretch			= TRUE
    }	
    image
    {
     function			= BOX
      recolorable		= TRUE
      shadow			= OUT
      state			= NORMAL
      file			= "Images/Shadows/entry-shadow-in-panel-blue.png" 
      border			= { 3,3,4,3 }
      stretch			= TRUE
    }
    image	
    {
      function			= BOX
      recolorable		= TRUE
      shadow			= OUT
      state			= ACTIVE
      file			= "Images/Shadows/entry-focus-panel-blue.png"
      border			= { 2,2,2,2 }
      stretch			= TRUE
    }
    image	
    {
