		orientation			= VERTICAL
	}
	
	image
	{
		function			= BOX
		detail				= "bar"
		file				= "Images/ProgressBar/progressbar-horiz.png"
		border				= { 2, 2, 2, 2 }
		stretch				= TRUE
		overlay_file                    = "Images/ProgressBar/over-progressbar-horiz.png"
